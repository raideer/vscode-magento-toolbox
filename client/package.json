{
  "name": "magento-toolbox",
  "displayName": "Magento Toolbox",
  "description": "Magento 2 code generation, inspection and utility tools",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.63.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raideer/vscode-magento-toolbox.git"
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "yarn compile:server && yarn compile:client",
    "compile:server": "cd server && tsc --outDir ../dist/",
    "compile:client": "webpack",
    "watch": "tsc -b -w server & webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "deploy:local": "vsce package -o dist/magento-toolbox.vsix --yarn && code --install-extension dist/magento-toolbox.vsix --force",
    "lint": "eslint src --ext ts",
    "prettier:fix": "prettier --write ."
  },
  "devDependencies": {
    "@githubocto/tailwind-vscode": "^1.0.4",
    "@types/ejs": "^3.1.0",
    "@types/glob": "^8.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/lodash-es": "^4.17.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "14.x",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^18.0.9",
    "@types/to-json-schema": "^0.2.1",
    "@types/validatorjs": "^3.15.0",
    "@types/vscode": "^1.63.0",
    "@types/vscode-webview": "^1.57.0",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^2.2.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "crc-32": "^1.2.2",
    "ejs": "^3.1.7",
    "formik": "^2.2.9",
    "indent-string": "^5.0.0",
    "informed": "^4.0.17",
    "jsdom": "^23.0.1",
    "lodash-es": "^4.17.21",
    "php-parser": "^3.1.2",
    "php-writer": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-loader": "^4.13.1",
    "react-select": "^5.8.0",
    "typescript-memoize": "^1.1.1",
    "validatorjs": "^3.22.1",
    "xml2js": "^0.5.0"
  },
  "resolutions": {
    "@types/react": "^17.0.37"
  }
}
